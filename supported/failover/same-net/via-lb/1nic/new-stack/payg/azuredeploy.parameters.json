{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "7.4.1.0",
    "parameters": {
        "adminUsername": {
            "value": "azureuser"
        },
        "authenticationType": {
            "value": "password"
        },
        "adminPasswordOrKey": {
            "value": ""
