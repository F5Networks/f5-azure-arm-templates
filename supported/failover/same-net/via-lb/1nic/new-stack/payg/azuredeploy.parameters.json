        "dnsLabel": {
            "value": ""
        },
        "instanceType": {
            "value": "Standard_DS2_v2"
        },
        "imageName": {
            "value": "Best1Gbps"
        },
        "bigIpVersion": {
            "value": "15.0.100000"
        },
        "bigIpModules": {
            "value": "ltm:nominal"
