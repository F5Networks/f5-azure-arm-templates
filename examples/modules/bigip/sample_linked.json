{
	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "9.0.0.0",
	"parameters": {
		"artifactLocation": {
			"defaultValue": "f5-azure-arm-templates/examples/",
			"metadata": {
				"description": "Location where modules folder is stored."
			},
			"type": "string"
		},
		"templateBaseUrl": {
			"defaultValue": "https://cdn.f5.com/product/cloudsolutions/",
			"metadata": {
				"description": "URL where templates are stored."
			},
			"type": "string"
		}
	},
	"variables": {
		"deploymentApiVersion": "2019-05-01"
	},
	"resources": [
		{
			"apiVersion": "[variables('deploymentApiVersion')]",
			"name": "bigipTemplate",
			"properties": {
				"mode": "Incremental",
				"parameters": {
					"adminUsername": {
						"value": "azureuser"
					},
					"appInsights": {
						"value": "bigip-app"
					},
					"cpuMetricName": {
						"value": "Percentage CPU"
					},
					"customAutoscaleRules": {
						"value": []
					},
					"customEmail": {
						"value": []
					},
					"dnsLabel": {
						"value": "appDns"
					},
					"image": {
						"value": "f5-networks:f5-big-ip-best:f5-bigip-virtual-edition-25m-best-hourly:15.1.002000"
					},
					"inboundMgmtNatPoolID": {
						"value": ""
					},
					"inboundSshNatPoolID": {
						"value": ""
					},
					"instanceHealthProbeID": {
						"value": ""
					},
					"instanceType": {
						"value": "Standard_DS2_v2"
					},
					"loadBalancerBackendAddressPoolsArray": {
						"value": []
					},
					"nsgID": {
						"value": ""
					},
					"provisionPublicIP": {
						"value": {}
					},
					"roleDefinitionId": {
						"value": ""
					},
					"runtimeConfig": {
						"value": ""
					},
					"scaleInCpuThreshold": {
						"value": 20
					},
					"scaleInThroughputThreshold": {
						"value": 10000000
					},
					"scaleInTimeWindow": {
						"value": 10
					},
					"scaleOutCpuThreshold": {
						"value": 80
					},
					"scaleOutThroughputThreshold": {
						"value": 20000000
					},
					"scaleOutTimeWindow": {
						"value": 10
					},
					"sshKey": {
						"value": ""
					},
					"subnetId": {
						"value": ""
					},
					"tagValues": {
						"value": {
							"application": "APP",
							"cost": "COST",
							"environment": "ENV",
							"group": "GROUP",
							"owner": "OWNER"
						}
					},
					"throughputMetricName": {
						"value": "Network In"
					},
					"useAvailabilityZones": {
						"value": "Yes"
					},
					"userAssignManagedIdentity": {
						"value": ""
					},
					"vmScaleSetMaxCount": {
						"value": 4
					},
					"vmScaleSetMinCount": {
						"value": 2
					},
					"vmssName": {
						"value": "bigip-vmss"
					}
				},
				"templateLink": {
					"contentVersion": "9.0.0.0",
					"uri": "[concat(parameters('templateBaseUrl'), parameters('artifactLocation'), 'modules/bigip/bigip.json')]"
				}
			},
			"type": "Microsoft.Resources/deployments"
		}
	],
	"outputs": {
		"appInsightsComponentID": {
			"type": "array",
			"value": "[reference('bigipTemplate').outputs.appInsightsComponentID.value]"
		},
		"appIps": {
			"type": "array",
			"value": "[reference('bigipTemplate').outputs.roleAssignmentID.value]"
		},
		"autoscaleSettingsID": {
			"type": "string",
			"value": "[reference('bigipTemplate').outputs.autoscaleSettingsID.value]"
		},
		"vmssID": {
			"type": "string",
			"value": "[reference('bigipTemplate').outputs.vmssID.value]"
		}
	}
}